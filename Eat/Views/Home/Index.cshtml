@model Eat.Models.ViewModels.StartIndexViewModel
@{
    /**/

    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Start slider  -->
<section id="aa-slider">
    <div class="aa-slider-area">
        <!-- Top slider -->
        <div class="aa-top-slider">
            @foreach (var item in Model.TopRestaurants)
            {
                <!-- Top slider single slide -->
                <div class="aa-top-slider-single">
                    <img src="@Url.Content(item.PathMainFoto)" alt="img">
                    <!-- Top slider content -->
                    <div class="aa-top-slider-content">
                        <span class="aa-top-slider-catg">@item.Name</span>
                        <h2 class="aa-top-slider-title">@item.Slogan</h2>
                        <p class="aa-top-slider-location"><i class="fa fa-map-marker"></i>@item.Address.ToStringShort()</p>
                        <span class="aa-top-slider-off">@item.Tables.Count столов</span>
                        <p class="aa-top-slider-price">@item.GoogleRestaurantDetails.Rating рейтинг</p>
                        <a href="@Url.Action("Details","Restaurant",new { id = item.Id } )" class="aa-top-slider-btn">Read More <span class="fa fa-angle-double-right"></span></a>
                    </div>
                    <!-- / Top slider content -->
                </div>
                <!-- / Top slider single slide -->
            }
        </div>
    </div>
</section>
<!-- End slider  -->
<!-- Advance Search -->
<section id="aa-advance-search">
    <div class="container">
        <div class="aa-advance-search-area">
            @using (Html.BeginForm("Search", "Search", FormMethod.Post))
            {
                @Html.AntiForgeryToken()
                <div class="form">
                    <div class="aa-advance-search-top">
                        <div class="row">
                            <div class="col-md-4">
                                <label class="">Where to looking for?</label>
                                <input class="form-control" type="text" name="Place" placeholder="Type Your Location">
                            </div>

                            <div class="col-md-2 ">
                                <label for="example-date-input" class="col-md-2 col-form-label">Date</label>
                                <input class="form-control" type="date" name="Date" value="" id="example-date-input">
                            </div>


                            <div class="col-md-2 ">
                                <label for="example-time-input" class="col-md-2 col-form-label">Time</label>
                                <input class="form-control" step="900" type="time" name="Time" value="" id="example-time-input">
                            </div>


                            <div class="col-md-2 ">
                                <label for="example-number-input" class="col-md-2 col-form-label">Persons</label>
                                <input class="form-control" type="number" name="NumberPersons" min="0" value="null" id="example-number-input">
                            </div>

                            <div class="col-md-2">
                                <div class="aa-single-advance-search">
                                    <input class="aa-search-btn" style="margin-top:20px" type="submit" value="Search">
                                </div>
                            </div>
                        </div>
                        <div id="accordion">
                            <h3>Click here to find the object, dish or igridient</h3>
                            <div class="">
                                <div class="row">
                                    <div class="col-md-6">
                                        <label class="">What to look for?</label>
                                        <input class="form-control" name="TextSearch" type="text" placeholder="Type Name">
                                    </div>
                                    <fieldset class="form-group col-md-6" id="fieldsetNameSearch">
